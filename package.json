{
  "name": "slither-backend",
  "version": "0.1.0",
  "private": true,
  "description": "Minimal Slither-like backend in TypeScript with fixed 30ms tick and WebSocket server",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "nanoid": "^5.0.7",
    "ws": "^8.18.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/node": "^22.7.5",
    "@types/ws": "^8.5.10",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3"
  }
}
